{
  "request" : {
    "urlPattern" : "/boards/(\\d+)",
    "method" : "PUT",
    "headers" : {
      "Content-Type" : {
        "equalTo" : "application/json;charset=UTF-8"
      }
    },
    "bodyPatterns" : [ {
      "matchesJsonPath" : "$[?(@.type == 'AIRCRAFT_CARRIER')]"
    }, {
      "matchesJsonPath" : "$.start[?(@.x == 0)]"
    }, {
      "matchesJsonPath" : "$.start[?(@.y == 0)]"
    }, {
      "matchesJsonPath" : "$.end[?(@.x == 0)]"
    }, {
      "matchesJsonPath" : "$.end[?(@.y == 4)]"
    } ]
  },
  "response" : {
    "status" : 200,
    "body" : "{\"id\":1,\"ships\":[{\"type\":\"AIRCRAFT_CARRIER\",\"start\":{\"x\":0,\"y\":0},\"end\":{\"x\":0,\"y\":4},\"sunk\":false,\"id\":1}],\"moves\":[]}",
    "headers" : {
      "Content-Type" : "application/json;charset=UTF-8"
    }
  }
}